{
  "manifest_version": 3,
  "name": "Extensão - Simulação Habitacional",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://*.caixa.gov.br/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "inpage.js"
      ],
      "matches": [
        "https://*.caixa.gov.br/*"
      ]
    }
  ]
}
