{
  "manifest_version": 3,
  "name": "MyInjector",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["caixa.gov.br/*"],
  "action": {},
  "background": {
    "service_worker": "dist/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["inpage.js"],
      "matches": ["https://*.caixa.gov.br/*"]
    }
  ]
}
